{
  "name": "@fsinfuhh/js_auth_mafiasi",
  "description": "JavaScript authentication library for working with Mafiasi",
  "license": "MIT",
  "version": "1.0.0",
  "repository": "git://github.com/fsinfuhh/js_auth_mafiasi.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "author": {
    "name": "Finn-Thorben Sell",
    "email": "dev@finn-thorben.me",
    "url": "https://finn-thorben.me"
  },
  "maintainers": [
    {
      "name": "Server-AG",
      "email": "ag-server@informatik.uni-hamburg.de"
    }
  ],
  "scripts": {
    "compile": "tsc -p .",
    "lint": "eslint --max-warnings 0 --fix .",
    "prepare": "yarn run lint && yarn run compile"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@openid/appauth": "^1.3.0",
    "jwt-js": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "typescript": "^4.1.3"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-non-null-assertion": "off"
    },
    "ignorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
